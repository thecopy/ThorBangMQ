writes = [];
writeAvgs = [];
reads = [];
readAvgs = [];
for i = 1:3
    file = strcat('test1.', num2str(i), '_server0_test.txt');
    data = csvread(file,0,1);
    data = data(5:55,:);
    
    writes(i, :) = data(:,1)';
    reads(i, :) = data(:,2)';
end

configs = [200 50 5];
errorbar(configs, mean(2.*writes'), std(2.*writes'),'r')
hold on
errorbar(configs, mean(reads'), std(reads'),'b')
hold off