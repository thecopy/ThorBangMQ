writes = [];
reads = [];
for i = 2:9
    file = strcat('test1.', num2str(i), '_server0_test.txt');
    data = csvread(file,0,1);
    
    pad_length = length(writes(i-1,:)) - length(data(:,1)');
    
    writes(i-1,:) = data(:,1)';
    reads(i-1,:) = data(:,2)';
end

configs = [200 175 150 125 100 75 50 25];
plot(configs, mean(writes))